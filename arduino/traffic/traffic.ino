int red=10;
int y=7;
int s=4;



void setup() {
  pinMode(red,OUTPUT);
  pinMode(red,OUTPUT);
  pinMode(red,OUTPUT);


}

void loop() {
 digitalWrite(red,HIGH);
 delay(5000);
 digitalWrite(red,LOW);
 digitalWrite(y,HIGH);
 delay(5000);
 digitalWrite(y,LOW);
 digitalWrite(s,HIGH);
 delay(5000);
 digitalWrite(s,LOW);
 
}
